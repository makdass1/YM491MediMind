name: dockercompose10224974978582138795
services:
  app.api:
    build:
      context: C:\Users\Mustafa\Desktop\GITHUB\YM491MediMind\..  \App.API
      dockerfile: App.API/dockerfile
    container_name: app_api_container
    depends_on:
      keycloak:
        condition: service_started
        required: true
      mssql:
        condition: service_started
        required: true
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ConnectionStrings__DefaultConnection: Server=mssql,1433;Database=MyDb;User Id=sa;Password=SizinGucluSifreniz1!;TrustServerCertificate=True;
    networks:
      default: null
    ports:
      - mode: ingress
        target: 8080
        published: "5127"
        protocol: tcp
  keycloak:
    command:
      - start-dev
      - --import-realm
    container_name: keycloak
    depends_on:
      mssql:
        condition: service_started
        required: true
    environment:
      KC_DB: mssql
      KC_DB_PASSWORD: SizinGucluSifreniz1!
      KC_DB_URL: jdbc:sqlserver://mssql:1433;databaseName=keycloak;encrypt=false;trustServerCertificate=true
      KC_DB_USERNAME: sa
      KC_HOSTNAME: localhost
      KEYCLOAK_ADMIN: admin
      KEYCLOAK_ADMIN_PASSWORD: admin
    image: quay.io/keycloak/keycloak:26.0
    networks:
      default: null
    ports:
      - mode: ingress
        target: 8080
        published: "8080"
        protocol: tcp
  mssql:
    container_name: keycloak-mssql
    environment:
      ACCEPT_EULA: "Y"
      MSSQL_PID: Express
      MSSQL_SA_PASSWORD: SizinGucluSifreniz1!
    image: mcr.microsoft.com/mssql/server:2022-latest
    networks:
      default: null
    ports:
      - mode: ingress
        target: 1433
        published: "1433"
        protocol: tcp
    volumes:
      - type: volume
        source: mssql_data
        target: /var/opt/mssql
        volume: {}
networks:
  default:
    name: dockercompose10224974978582138795_default
volumes:
  mssql_data:
    name: dockercompose10224974978582138795_mssql_data